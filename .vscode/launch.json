{
  "version": "0.2.0",
  "configurations": [
     {
        "name": "GDB Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/go.exe",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "C:/tools/msys64/mingw64/bin/gdb.exe",
        "setupCommands": [
           {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
           }
        ],
        "preLaunchTask": "build",
        "internalConsoleOptions": "openOnSessionStart"
     }
  ]
}
